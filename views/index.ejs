<body class="blue lighten-5">
    <h3 class="condensed"> <center> Uber for logistic Management </center></h3>
    <br>
    <br>
    <div>
        <div class="container">
            <center>
            <div class="row valign-wrapper">
                <div class="col s6 offset-s3 valign">
                    <div class="card white">
                        <div class="card-content black-text">
                            <span class="card-title"> Enter your details </span>
                            <form method="post" action="/loggedIn">
                                Username:
                                <input type="text" name="uname">
                                <br>
                                <br> 
                                Password:
                                <input type="password" name="pass">
                                <br>
                        </div>
                        <div class="card-action">
                            <div class="row">
                                <div class="col s6">
                                    <button class="btn black" type="reset" value="Reset">Reset</button>
                                </div>
                                <div class"col s3 offset-s12">
                                    <button class="btn light-blue darken-3" type="submit" value="Submit" ng-click="submit()">Submit
                                        <i class="material-icons right">send</i>
                                    </button>
                                </div>
                            </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            </center>
